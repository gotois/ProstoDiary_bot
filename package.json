    "http-auth": "4.1.2",
    "imap": "0.8.19",
    "ioredis": "4.16.2",
    "jayson": "3.2.0",
    "jimp": "0.14.0",
    "jose": "1.25.0",
    "jsonld": "3.1.1",
    "jsonld-signatures": "5.0.1",
    "jsqr": "1.2.0",
    "mailparser": "2.7.7",
    "memjs": "1.2.2",
    "music-metadata": "6.3.7",
    "node-fetch": "2.6.0",
    "node-telegram-bot-api": "0.40.0",
    "oidc-provider": "6.23.5",
    "openapi-jsonrpc-jsdoc": "1.0.3",
    "openpgp": "4.10.1",
    "openweather-apis": "4.2.0",
    "qrcode": "1.4.4",
    "qs": "6.9.3",
    "schema.org": "3.1.1",
    "slonik": "22.4.11",
    "slonik-interceptor-query-logging": "1.3.8",
    "snyk": "^1.362.1",
    "speakeasy": "2.0.0",
    "uuid": "7.0.3",
    "validator": "13.0.0",
    "winston": "3.2.1",
    "winston-daily-rotate-file": "4.4.2",
    "yauzl": "2.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@roarr/cli": "3.2.0",
    "@sentry/cli": "1.55.2",
    "@types/express": "4.17.8",
    "ava": "3.12.1",
    "codecov": "4.0.0-0",
    "commitizen": "4.1.2",
    "cross-env": "7.0.2",
    "cz-conventional-changelog": "3.2.0",
    "dependency-cruiser": "8.2.0",
    "eslint": "7.5.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-ava": "10.5.0",
    "eslint-plugin-jsdoc": "30.3.0",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-sql": "1.1.1",
    "eslint-plugin-unicorn": "21.0.0",
    "get-port-cli": "2.0.0",
    "husky": "3.1.0",
    "inquirer": "7.3.3",
    "lint-staged": "10.2.11",
    "markdownlint-cli": "0.23.2",
    "ngrok": "3.2.7",
    "nodemon": "2.0.4",
    "npm-check-updates": "4.1.2",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "prettier": "2.0.5",
    "supertest": "4.0.2",
    "telegram-test-api": "2.4.2"
  },
  "snyk": true
}
