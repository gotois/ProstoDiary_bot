{
  "name": "prosto-diary",
  "version": "0.0.0-development",
  "description": "ProstoDiary: your personal all-in-one virtual assistant",
  "publisher": "goto Interactive Software",
  "homepage": "https://prosto-diary.gotointeractive.com",
  "main": "./src/server.js",
  "workspaces": [
    "src/*",
    "scripts/*"
  ],
  "private": false,
  "bin": "./scripts/index",
  "files": [
    "src",
    "scripts"
  ],
  "scripts": {
    "postinstall": "ncu ; npm run clean",
    "commit": "git-cz",
    "pretest": "npm run lint -- --quiet",
    "test": "nyc --reporter=text ava tests/unit/index.js tests/e2e/index.js --serial",
    "start": "node ./src/server.js",
    "start:jsonld": "./scripts/jsonld",
    "build": "",
    "depcruise": "depcruise --include-only ^src --output-type dot src | dot -T svg > dependencygraph.svg",
    "heroku-postbuild": "./scripts/sendgrid-update-webhook",
    "debug": "cross-env NODE_ENV=production node --inspect ./scripts/index",
    "clean": "rm -rf .nyc_output && rm -rf logs/ && ava tests/e2e/index.js --serial --reset-cache",
    "check": "node -r dotenv/config src/server.js --check",
    "dev": "killall ngrok ; cross-env NODE_ENV=development PORT=9000 ROARR_LOG=true ./scripts/index",
    "dev:debug": "cross-env NODE_ENV=development --verbose --inspect ./scripts/index",
    "lint": "eslint scripts/* src/*.js src/**/*.js src/**/**/*.js tests/**/*.js --quiet",
    "lint:dependency": "depcruise --validate .dependency-cruiser.js src",
    "unit": "ava tests/unit/index.js",
    "unit:watch": "npm run unit -- --watch",
    "e2e": "ava tests/e2e/index.js --serial",
    "e2e:fast": "cross-env FAST_TEST=FAST_TEST ava tests/e2e/index.js",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "docs:serve": "cd docs/ ; bundle exec jekyll serve",
    "docs:openapi": "node ./scripts/openapi",
    "prepare": "if [ \"$NODE_ENV\" = \"TRAVIS_CI\" ]; then echo skip prepare script; else npm run snyk-protect; fi",
    "snyk-protect": "snyk protect"
  },
  "engineStrict": true,
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "os": [
    "!win32"
  ],
  "cpu": [
    "x64"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gotois/ProstoDiary_bot.git"
  },
  "keywords": [
    "prostodiary",
    "prosto diary",
    "virtual assistant",
    "personal bot"
  ],
  "author": {
    "name": "gotois",
    "email": "no-reply@gotointeractive.com"
  },
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/gotois/ProstoDiary_bot/issues",
    "email": "support@gotointeractive.com"
  },
  "maintainers": [
    {
      "email": "denis@baskovsky.ru",
      "name": "Denis Baskovsky",
      "url": "http://denis.baskovsky.ru/"
    }
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@google-cloud/language": "3.8.0",
    "@google-cloud/speech": "3.6.0",
    "@google-cloud/translate": "5.3.0",
    "@google-cloud/vision": "1.11.0",
    "@sendgrid/mail": "6.5.4",
    "@sentry/node": "5.14.2",
    "accesscontrol": "2.2.1",
    "archiver": "3.1.1",
    "body-parser": "1.19.0",
    "connect-pg-simple": "6.1.0",
    "coralogix-logger-winston": "1.0.11",
    "crypto-ld": "3.7.0",
    "crypto-random-string": "3.2.0",
    "date-fns": "2.11.0",
    "delay": "4.3.0",
    "dialogflow": "1.2.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-openapi-validator": "3.10.0",
    "express-session": "1.17.0",
    "express-winston": "4.0.3",
    "eyo-kernel": "2.5.6",
    "fast-xml-parser": "3.16.0",
    "file-type": "14.1.4",
    "franc": "5.0.0",
    "grant-express": "4.7.0",
    "helmet": "3.21.3",
    "http-auth": "3.2.4",
    "imap": "0.8.19",
    "ioredis": "4.16.0",
    "jayson": "3.2.0",
    "jimp": "0.9.5",
    "jose": "1.25.0",
    "jsonld": "2.0.2",
    "jsonld-signatures": "4.6.0",
    "jsqr": "1.2.0",
    "mailparser": "2.7.7",
    "music-metadata": "6.3.6",
    "node-telegram-bot-api": "0.40.0",
    "oidc-provider": "6.20.1",
    "openpgp": "4.10.1",
    "openweather-apis": "4.2.0",
    "qrcode": "1.4.4",
    "qs": "6.9.1",
    "schema.org": "3.1.1",
    "slonik": "22.4.3",
    "snyk": "^1.290.1",
    "speakeasy": "2.0.0",
    "uuid": "7.0.2",
    "validator": "12.2.0",
    "winston": "3.2.1",
    "yauzl": "2.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@sentry/cli": "1.51.1",
    "ava": "2.4.0",
    "codecov": "4.0.0-0",
    "commitizen": "4.0.3",
    "cross-env": "7.0.2",
    "cz-conventional-changelog": "3.1.0",
    "dependency-cruiser": "8.0.1",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-ava": "10.2.0",
    "eslint-plugin-jsdoc": "22.0.1",
    "eslint-plugin-json": "2.1.1",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-sql": "1.1.1",
    "eslint-plugin-unicorn": "17.2.0",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "ngrok": "3.2.7",
    "nodemon": "2.0.2",
    "npm-check-updates": "4.0.4",
    "nyc": "15.0.0",
    "openapi-jsonrpc-jsdoc": "1.0.3",
    "prettier": "1.19.1",
    "telegram-test-api": "2.4.2"
  },
  "snyk": true
}
