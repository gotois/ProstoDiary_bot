{
  "name": "prosto-diary",
  "version": "0.0.0-development",
  "description": "ProstoDiary: your personal all-in-one virtual assistant",
  "publisher": "goto Interactive Software",
  "homepage": "https://prosto-diary.gotointeractive.com",
  "main": "src/app/server.js",
  "workspaces": [
    "src/*",
    "scripts/*"
  ],
  "directories": {
    "src": "src",
    "docs": "docs"
  },
  "private": false,
  "bin": "./scripts/index",
  "files": [
    "src",
    "scripts"
  ],
  "scripts": {
    "postinstall": "scripts/post-install.sh",
    "commit": "git-cz",
    "pretest": "run-s --silent \"lint -- --quiet\" lint:dependency",
    "test": "nyc --reporter=text run-s test:unit test:e2e",
    "test:unit": "ava tests/unit/index.js",
    "test:e2e": "ava tests/e2e/index.js --serial",
    "test:e2e:fast": "cross-env FAST_TEST=FAST_TEST ava tests/e2e/index.js",
    "start": "node src/app/server.js",
    "start:debug": "cross-env NODE_ENV=production node --inspect -r dotenv/config src/app/server.js",
    "start:new-assistant": "./scripts/add-assistant",
    "dev": "killall ngrok ; PORT=$(get-port 9000) ; cross-env NODE_ENV=development PORT=$PORT ROARR_LOG=true ./scripts/index | roarr pretty-print",
    "dev:debug": "killall ngrok ; PORT=$(get-port 9000) ; cross-env NODE_ENV=development HOST=http://127.0.0.1 PORT=$PORT node --inspect -r dotenv/config src/app/server.js",
    "dev:docs": "cd docs/ ; bundle exec jekyll serve",
    "heroku-prebuild": "",
    "heroku-postbuild": "./scripts/sendgrid-update-webhook",
    "clean": "npm-run-all clean:report clean:logs clean:test-cache",
    "clean:report": "rm -rf .nyc_output",
    "clean:logs": "rm -rf logs/",
    "clean:test-cache": "ava reset-cache tests/e2e/index.js --serial",
    "lint": "eslint scripts/* src/**/**/**/*.js src/**/**/*.js src/**/*.js tests/**/*.js",
    "lint:dependency": "depcruise --validate .dependency-cruiser.js src tests",
    "lint:markdown": "markdownlint 'docs/**/*.md' --ignore 'docs/_site' --ignore 'docs/index.md'",
    "report:dependency": "mkdir -p reports && depcruise --include-only ^src --output-type dot src | dot -T svg > reports/dependencygraph.svg",
    "report:coverage": "mkdir -p reports && nyc report --reporter=text-lcov > reports/coverage.lcov && codecov",
    "prepare": "npm run snyk-protect",
    "snyk-protect": "scripts/prepare.sh"
  },
  "engineStrict": true,
  "engines": {
    "node": "14.x",
    "npm": "6.x"
  },
  "os": [
    "!win32"
  ],
  "cpu": [
    "x64"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gotois/ProstoDiary_bot.git"
  },
  "keywords": [
    "prostodiary",
    "prosto diary",
    "virtual assistant",
    "personal bot"
  ],
  "author": {
    "name": "gotois",
    "email": "no-reply@gotointeractive.com"
  },
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/gotois/ProstoDiary_bot/issues",
    "email": "support@gotointeractive.com"
  },
  "maintainers": [
    {
      "email": "denis@baskovsky.ru",
      "name": "Denis Baskovsky",
      "url": "http://denis.baskovsky.ru/"
    }
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@google-cloud/language": "3.8.0",
    "@google-cloud/speech": "3.6.0",
    "@google-cloud/storage": "4.7.0",
    "@google-cloud/translate": "5.3.0",
    "@google-cloud/vision": "1.11.0",
    "@sendgrid/mail": "7.2.1",
    "@sentry/node": "5.15.4",
    "Haraka": "2.8.25",
    "accesscontrol": "2.2.1",
    "archiver": "3.1.1",
    "body-parser": "1.19.0",
    "boxen": "4.2.0",
    "bullmq": "1.8.13",
    "cheerio": "1.0.0-rc.3",
    "connect-pg-simple": "6.1.0",
    "coralogix-logger-winston": "1.0.11",
    "crypto-ld": "3.7.0",
    "crypto-random-string": "3.2.0",
    "csurf": "1.11.0",
    "date-fns": "2.12.0",
    "dialogflow": "1.2.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-openapi-validator": "3.11.0",
    "express-session": "1.17.1",
    "express-winston": "4.0.3",
    "eyo-kernel": "2.5.6",
    "fast-xml-parser": "3.16.0",
    "file-type": "14.1.4",
    "form-data": "3.0.0",
    "franc": "5.0.0",
    "grant-express": "4.7.0",
