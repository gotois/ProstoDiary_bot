{
  "name": "prosto-diary",
  "description": "Global virtual assistant",
  "version": "1.0.0",
  "homepage": "https://prosto-diary.gotointeractive.com",
  "main": "./src/index.js",
  "man": [],
  "private": false,
  "bin": {
    "prosto-diary": "./scripts/index"
  },
  "files": [
    "src/",
    "tests/"
  ],
  "directories": {
    "data": "./data",
    "e2e": "./tests/e2e"
  },
  "scripts": {
    "postinstall": "npx ncu",
    "preversion": "npm run test",
    "commit": "npx git-cz",
    "clean": "rm -rf .nyc_output && rm coverage.lcov && ava tests/e2e/index.js --serial --reset-cache",
    "dev": "nodemon ./scripts/index && npm run unit:watch",
    "dev:debug": "nodemon --inspect ./scripts/index",
    "start": "./scripts/index",
    "start:debug": "cross-env NODE_ENV=production node --inspect ./scripts/index",
    "lint": "eslint src/*.js src/**/*.js tests/**/*.js",
    "test": "nyc --reporter=text ava tests/unit/index.js tests/e2e/index.js --serial",
    "unit": "cross-env SERVER_NAME=TEST-AVA-SERVER TELEGRAM_TOKEN=123456 SALT_PASSWORD=123456 ava tests/unit/index.js",
    "unit:watch": "npm run unit -- --watch",
    "e2e": "ava tests/e2e/index.js --serial",
    "e2e:fast": "cross-env FAST_TEST=FAST_TEST SERVER_NAME=TEST-AVA-SERVER TELEGRAM_TOKEN=123456 SALT_PASSWORD=123456 ava tests/e2e/index.js",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "docs:serve": "cd docs/ ; jekyll serve"
  },
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "os": [
    "!win32"
  ],
  "cpu": [
    "x64"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gotois/ProstoDiary_bot.git"
  },
  "keywords": [
    "prostodiary",
    "prosto diary",
    "virtual assistant"
  ],
  "author": {
    "name": "Denis Baskovsky",
    "url": "http://denis.baskovsky.ru/"
  },
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/gotois/ProstoDiary_bot/issues",
    "email": "support@gotointeractive.com"
  },
  "maintainers": [
    {
      "email": "denis@baskovsky.ru",
      "name": "Denis Baskovsky"
    }
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@google-cloud/language": "~3.2.1",
    "@google-cloud/speech": "~3.2.1",
    "@google-cloud/translate": "~4.1.0",
    "@google-cloud/vision": "~1.1.2",
    "@sendgrid/mail": "~6.4.0",
    "archiver": "~3.0.0",
    "compromise": "~11.13.2",
    "coralogix-logger-winston": "~1.0.10",
    "date-fns": "~1.30.1",
    "dialogflow": "~0.10.1",
    "dotenv": "~8.0.0",
    "fast-xml-parser": "~3.12.16",
    "franc": "~4.1.0",
    "jimp": "~0.6.4",
    "jsqr": "~1.2.0",
    "music-metadata": "~4.1.1",
    "node-telegram-bot-api": "~0.30.0",
    "openweather-apis": "~4.0.0",
    "pg": "~7.11.0",
    "plotly": "~1.0.6",
    "winston": "~3.2.1",
    "yauzl": "~2.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "~8.0.0",
    "ava": "~2.1.0",
    "codecov": "~4.0.0-0",
    "commitizen": "~3.1.1",
    "cross-env": "~5.2.0",
    "cz-conventional-changelog": "~2.1.0",
    "eslint": "~6.0.1",
    "eslint-config-prettier": "~6.0.0",
    "eslint-plugin-ava": "~7.1.0",
    "eslint-plugin-jsdoc": "~8.6.1",
    "eslint-plugin-node": "~9.1.0",
    "eslint-plugin-prettier": "~3.1.0",
    "husky": "~2.5.0",
    "nodemon": "~1.19.1",
    "npm-check-updates": "~3.1.12",
    "nyc": "~14.1.1",
    "prettier": "~1.18.2",
    "telegram-test-api": "~2.2.1"
  }
}
