{
  "name": "prosto-diary",
  "version": "0.0.0-development",
  "description": "ProstoDiary: Global virtual assistant",
  "publisher": "goto Interactive Software",
  "homepage": "https://prosto-diary.gotointeractive.com",
  "main": "./src/index.js",
  "workspaces": [
    "src/*",
    "scripts/*"
  ],
  "private": false,
  "bin": "./scripts/index",
  "files": [
    "src",
    "scripts"
  ],
  "scripts": {
    "postinstall": "ncu ; npm run clean",
    "commit": "git-cz",
    "pretest": "npm run lint",
    "test": "nyc --reporter=text ava tests/unit/index.js tests/e2e/index.js --serial",
    "start": "./scripts/index",
    "start:debug": "cross-env NODE_ENV=production node --inspect ./scripts/index",
    "start:naturalize": "./scripts/naturalize",
    "clean": "rm -rf .nyc_output && ava tests/e2e/index.js --serial --reset-cache",
    "dev": "cross-env NODE_ENV=development nodemon ./scripts/index",
    "dev:debug": "cross-env NODE_ENV=development nodemon --verbose --inspect ./scripts/index",
    "lint": "eslint src/*.js src/**/*.js src/**/**/*.js tests/**/*.js",
    "unit": "ava tests/unit/index.js",
    "unit:watch": "npm run unit -- --watch",
    "e2e": "ava tests/e2e/index.js --serial",
    "e2e:fast": "cross-env FAST_TEST=FAST_TEST ava tests/e2e/index.js",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "docs:serve": "cd docs/ ; jekyll serve",
    "prepare": "npm run snyk-protect",
    "snyk-protect": "snyk protect"
  },
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "os": [
    "!win32"
  ],
  "cpu": [
    "x64"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gotois/ProstoDiary_bot.git"
  },
  "keywords": [
    "prostodiary",
    "prosto diary",
    "virtual assistant",
    "personal bot"
  ],
  "author": {
    "name": "Denis Baskovsky",
    "url": "http://denis.baskovsky.ru/"
  },
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/gotois/ProstoDiary_bot/issues",
    "email": "support@gotointeractive.com"
  },
  "maintainers": [
    {
      "email": "denis@baskovsky.ru",
      "name": "Denis Baskovsky"
    }
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@google-cloud/language": "3.2.6",
    "@google-cloud/speech": "3.2.6",
    "@google-cloud/translate": "4.1.3",
    "@google-cloud/vision": "1.4.0",
    "@sendgrid/mail": "6.4.0",
    "archiver": "3.1.1",
    "coralogix-logger-winston": "1.0.10",
    "date-fns": "2.4.1",
    "dialogflow": "0.10.3",
    "dotenv": "8.1.0",
    "eyo-kernel": "2.5.6",
    "fast-xml-parser": "3.12.20",
    "file-type": "12.3.0",
    "franc": "4.1.0",
    "jimp": "0.8.4",
    "jsqr": "1.2.0",
    "mail-notifier": "0.5.0",
    "music-metadata": "4.8.0",
    "node-telegram-bot-api": "0.30.0",
    "openweather-apis": "4.0.0",
    "pg": "7.12.1",
    "plotly": "1.0.6",
    "speakeasy": "2.0.0",
    "todoist-js": "0.3.1",
    "validator": "11.1.0",
    "winston": "3.2.1",
    "wolfram-alpha-api": "git+https://github.com/qertis/wolfram-alpha-api.git",
    "yauzl": "2.10.0",
    "snyk": "1.230.5"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "ava": "2.4.0",
    "codecov": "4.0.0-0",
    "commitizen": "4.0.3",
    "cross-env": "6.0.3",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "6.5.1",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-ava": "9.0.0",
    "eslint-plugin-jsdoc": "15.9.10",
    "eslint-plugin-json": "1.4.0",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-unicorn": "12.0.1",
    "husky": "3.0.7",
    "lint-staged": "9.4.0",
    "nodemon": "1.19.3",
    "npm-check-updates": "3.1.23",
    "nyc": "14.1.1",
    "prettier": "1.18.2",
    "telegram-test-api": "2.3.1"
  },
  "snyk": true
}
